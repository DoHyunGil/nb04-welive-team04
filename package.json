{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run prisma:reset && npm run prisma:migrate && node --experimental-vm-modules ./node_modules/.bin/jest --config jest.config.cjs",
    "test:custom": "npm run prisma:reset && npm run prisma:migrate && node --experimental-vm-modules ./node_modules/.bin/jest --config jest.config.cjs --testPathPatterns=\"auth.service.test|auth.repository.test|user.repository.test|admin.repository.test|comment.repository.test|comment.e2e.test\"",
    "test:repository": "npm run prisma:reset && npm run prisma:migrate && node --experimental-vm-modules ./node_modules/.bin/jest --config jest.config.cjs --testPathPatterns=\"repository.test\"",
    "dev": "tsx watch ./src/main.ts",
    "start": "node ./dist/src/main.js",
    "build": "tsc",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "prisma:reset": "npx dotenv -e .env.test -- prisma migrate reset --force",
    "prisma:migrate": "npx dotenv -e .env.test -- prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "type": "module",
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.957.0",
    "@aws-sdk/s3-request-presigner": "^3.957.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "express": "^5.1.0",
    "http-errors": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "node-cron": "^4.2.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.7.3",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  }
}
